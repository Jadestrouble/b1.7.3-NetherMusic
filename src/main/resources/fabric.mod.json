{
  "schemaVersion": 1,
  "id": "nethermusic",
  "version": "${version}",

  "name": "Nether Music",
  "description": "A mod for Minecraft Beta 1.7.3 that adds Minecraft Volume Beta Nether Music to play in the nether",
  "authors": [
    "Jadestrouble"
  ],
  "contact": {
    "homepage": "https://glass-launcher.net/",
    "sources": "https://github.com/Jadestrouble/b1.7.3-NetherMusic"
  },

  "license": "CC0-1.0",
  "icon": "assets/nethermusic/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus":[
    ],
    "stationapi:event_bus_client": [
      "jadestrouble.nethermusic.sounds.NetherMusicSoundManager"
    ],
    "stationapi:event_bus_server": [
    ]
  },
  "mixins": [
    "nethermusic.mixins.json"
  ],
  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
